
# From https://gist.github.com/ruediger/5647207
*.tex   diff=tex
*.bib   diff=bibtex
*.c     diff=cpp
*.h     diff=cpp
*.c++   diff=cpp
*.h++   diff=cpp
*.cpp   diff=cpp
*.hpp   diff=cpp
*.cc    diff=cpp
*.hh    diff=cpp
*.m     diff=matlab
*.py    diff=python
*.rb    diff=ruby
*.php   diff=php
*.pl    diff=perl
*.pm    diff=perl
*.html  diff=html
*.xhtml diff=html
*.f     diff=fortran
*.js    diff=js

